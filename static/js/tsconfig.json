{
    "compilerOptions": {
      "target": "ES6",             // 编译为 ES6
      "module": "commonjs",        // 使用 CommonJS 模块化规范
      "declaration": false,        // 不生成额外的声明文件
      "outDir": "./dist",          // 输出文件夹
      "rootDir": "./src",          // 输入文件夹
      "strict": true               // 启用严格类型检查
    },
    "include": ["src/**/*.ts", "src/imageElements.js"],     // 包含的文件
    "exclude": ["node_modules"]     // 排除的文件
  }
  